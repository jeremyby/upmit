

$('.modal').modal('hide')

if ($('#edit-commit').length) {
  $('.activity[data-type="Commit"][data-id="<%= @commit.id %>"]').html('<%= j render @commit %>')
}


$('.activities .activity .edit a').on('click', upmit.click_commit_edit)


<% if @error.present? %>
  upmit.message('<%= @error %>', 'alert')
<% else %>
  upmit.message('The entry was successfully updated.', 'notice')
<% end %>

upmit.stop_spinner()